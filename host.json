{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableDependencyTracking": true
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "telemetryMode": "OpenTelemetry",
  "durableTask": {
    "maxConcurrentActivityFunctions": 4,
    "maxConcurrentOrchestratorFunctions": 1
  }
}